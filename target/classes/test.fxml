<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.saxsys.examples.ExampleOneStarter">
    <children>
        <TableView fx:id="tableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   prefHeight="200.0" prefWidth="200.0" VBox.vgrow="SOMETIMES">

        </TableView>
        <HBox alignment="CENTER" spacing="50.0">
            <children>
                <Button mnemonicParsing="false" onAction="#addData" text="add data" />
                <Button mnemonicParsing="false" onAction="#clearTable" text="clear table" />
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </HBox>

    </children>
</VBox>
